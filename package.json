{
  "name": "gimme_readme",
  "version": "0.0.1",
  "description": "gimme_readme is a command-line tool that uses AI to generate a README.md file that explains a given source code file.",
  "author": "Peter Wan",
  "license": "MIT",
  "main": "_gr.js",
  "bin": {
    "gr": "./src/_gr.js"
  },
  "type": "module",
  "scripts": {
    "test": "jest -c jest.config.js --runInBand",
    "lint": "eslint --config eslint.config.mjs \"./src/**/*.js\" \"tests/**/*.js\"",
    "format": "prettier --write \"./src/**/*.js\" \"tests/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peterdanwan/gimme_readme.git"
  },
  "keywords": [
    "ai",
    "chat-completion",
    "learning",
    "README.md",
    "aryan"
  ],
  "bugs": {
    "url": "https://github.com/peterdanwan/gimme_readme/issues"
  },
  "homepage": "https://github.com/peterdanwan/gimme_readme#readme",
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0"
  }
}
